    <view bindtouchend="touchEnd" bindtouchstart="touchStart" class="container">
        <!--one-->
        <view class="one {{ view[0].in }} {{ view[0].out }}" style="width: 100%; height: 100%">
            <image class="one-one {{ one_one }}" src="https://7465-teach-1258324355.tcb.qcloud.la/tab/12.jpg"></image>
            <image class="one-two {{ one_two }}" src="https://7465-teach-1258324355.tcb.qcloud.la/tab/2.png"></image>
