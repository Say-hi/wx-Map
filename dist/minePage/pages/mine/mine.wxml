            <view class="two-five {{ two_five }}">广州·前端</view>
            <view catchtap="showEig" class="two-six {{ two_six }}"></view>
            <view catchtap="showEig" class="two-sev {{ two_sev }}"></view>
            <image hidden="{{ !two_eig_hide }}" catchtap="hideEig" class="two-eig {{ two_eig }}" src="https://7465-teach-1258324355.tcb.qcloud.la/tab/18.png"></image>
            <!--<view class="two-five {{ two_five }}">设计师</view>-->
        </view>
        <!--three-->
        <view class="three {{ view[2].in }} {{ view[2].out }}" style="width: 100%; height: 100%">
            <image class="three-one {{ three_one }}" src="https://7465-teach-1258324355.tcb.qcloud.la/tab/19.png"></image>
            <image class="three-two {{ three_two }}" src="https://7465-teach-1258324355.tcb.qcloud.la/tab/19.png"></image>
