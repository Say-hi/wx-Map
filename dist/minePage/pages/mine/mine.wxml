            <image class="three-four {{ three_four }}" src="https://7465-teach-1258324355.tcb.qcloud.la/tab/19.png"></image>
            <image mode="widthFix" catchtap="myImg" class="three-five {{ three_five }}" src="https://c.jiangwenqiang.com/api/my.jpg"></image>
            <!--<view class="three-six {{ three_six }}">点击预览保存图片，微信扫码添加</view>-->
            <view catchtap='copyText' class="three-sev {{ three_six }}">GitHub:https://github.com/Say-hi/wx-Map</view>
            <!--<contact-button type='default-light' class='haha' size='25'></contact-button>-->
        </view>
